# AWS/Wasabi S3 Credentials
# Copy this file to .env and fill in your actual credentials
# IMPORTANT: Never commit .env to version control

# Wasabi Access Key ID
AWS_ACCESS_KEY_ID=your_wasabi_access_key_here

# Wasabi Secret Access Key
AWS_SECRET_ACCESS_KEY=your_wasabi_secret_key_here

# AWS Region (for Wasabi, typically us-east-1, us-east-2, us-west-1, or eu-central-1)
AWS_DEFAULT_REGION=us-east-1

# Optional: Override config file location
# BACKUP_CONFIG_FILE=/path/to/custom/config.json
